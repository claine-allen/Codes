
package theboisproject;

public class MaxOfN {
    public static void main(String args[]){
        int a[]={1,32,45,36,22,57,575,32,462,25};
        int result=a[0];
        for(int i=1;i<a.length;i++){
            if(a[i]>result){
                result=a[i];
            }
        }System.out.println(result);
    }
}
